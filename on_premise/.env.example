# .env.example - Safe to commit (no real values!)

# Airflow
AIRFLOW_UID=50000
AIRFLOW_IMAGE_NAME=apache/airflow:3.1.0
AIRFLOW_PROJ_DIR=.
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=change_me_in_production

# PostgreSQL Warehouse
POSTGRES_WAREHOUSE_PORT=5432
POSTGRES_WAREHOUSE_DB=warehouse_db
POSTGRES_WAREHOUSE_USER=warehouse_user
POSTGRES_WAREHOUSE_PASSWORD=change_me

# PostgreSQL Airflow
POSTGRES_AIRFLOW_USER=airflow
POSTGRES_AIRFLOW_PASSWORD=change_me
POSTGRES_AIRFLOW_DB=airflow

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=change_me